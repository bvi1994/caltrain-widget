    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.0",
    "path": "^0.12.7"
  }
}
